{
  "manifest_version": 3,
  "name": "Bring Pinned Tabs",
  "version": "0.1.0",
  "description": "__MSG_extDes__",
  "homepage_url": "https://github.com/LightAPIs/bring-pinned-tabs",
  "icons": {
    "16": "icons/icon_16.png",
    "32": "icons/icon_32.png",
    "48": "icons/icon_48.png",
    "128": "icons/icon_128.png"
  },
  "default_locale": "en",
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_title": "Bring Pinned Tabs",
    "default_popup": "popup.html"
  },
  "permissions": ["storage"]
}
