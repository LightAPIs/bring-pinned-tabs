{
  "name": "bring-pinned-tabs",
  "version": "0.1.0",
  "description": "Allows you to move or copy pinned tabs to a new window when creating it.",
  "license": "MIT",
  "private": true,
  "scripts": {
    "watch": "webpack --mode=development --watch --config config/webpack.config.js",
    "build": "webpack --mode=production --config config/webpack.config.js",
    "format": "prettier --write --ignore-unknown \"{config,public,src}/**/*.{html,css,js,ts,json}\"",
    "postinstall": "patch-package"
  },
  "devDependencies": {
    "@types/chrome": "^0.0.206",
    "copy-webpack-plugin": "^10.2.4",
    "css-loader": "^6.7.3",
    "file-loader": "^6.2.0",
    "mini-css-extract-plugin": "^2.7.2",
    "patch-package": "^6.5.1",
    "prettier": "^2.8.2",
    "ts-loader": "^9.2.8",
    "typescript": "^4.6.3",
    "webpack": "^5.75.0",
    "webpack-cli": "^4.10.0",
    "webpack-merge": "^5.8.0"
  }
}
